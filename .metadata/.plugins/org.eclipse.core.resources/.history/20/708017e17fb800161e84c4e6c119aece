var connection = require('./module/mod_mysqlConn');

var opStatus = function(status) {
	this.status = status;

	this.getStatus = function() {
		connection.query('SELECT * FROM T000MCL;', function(err, result) {
			var st = result[0].OP_STATUS;
		})
		connection.end();
		return st;
	}
}

module.exports = opStatus;